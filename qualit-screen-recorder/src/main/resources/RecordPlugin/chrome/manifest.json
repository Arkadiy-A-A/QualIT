{
   "background": {"page": "background.html"},
   "browser_action": {
      "default_icon": {
         "128": "images/icons/128x128.png",
         "16": "images/icons/16x16.png",
         "48": "images/icons/48x48.png"
      },
      "default_title": "__MSG_appName__"
   },
   "commands": {
      "pause_video": {
         "description": "Pause video recording",
         "suggested_key": {
            "default": "Ctrl+Shift+0",
            "mac": "Command+Shift+0"
         }
      },
      "start_desktop_video": {
         "description": "Start record desktop",
         "suggested_key": {
            "default": "Ctrl+Shift+8",
            "mac": "Command+Shift+8"
         }
      },
      "start_tab_video": {
         "description": "Start record tab/page",
         "suggested_key": {
            "default": "Ctrl+Shift+7",
            "mac": "Command+Shift+7"
         }
      },
      "stop_video": {
         "description": "Stop video recording",
         "suggested_key": {
            "default": "Ctrl+Shift+9",
            "mac": "Command+Shift+9"
         }
      }
   },
   "content_scripts": [{
      "js": ["js/hotkeys.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
   }],
   "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
   "default_locale": "ru",
   "description": "__MSG_appDesc__",
   "externally_connectable": {"matches": [
      "*://*.google.com/*",
      "*://*.yandex.ru/*"
   ]},
   "homepage_url": "https://ibs-qa.ru/",
   "icons": {
      "128": "images/icons/128x128.png",
      "16": "images/icons/16x16.png",
      "48": "images/icons/48x48.png"
   },
   "incognito": "spanning",
   "key": "cffifjgabjalihmjiojbennijblkhccc",
   "manifest_version": 2,
   "minimum_chrome_version": "43",
   "name": "__MSG_appName__",
   "offline_enabled": true,
   "omnibox": {"keyword": "http://ibs-qa/"},
   "permissions": [
      "*://*/*",
      "http://*.google.com/",
      "<all_urls>",
      "downloads",
      "clipboardWrite",
      "clipboardRead",
      "tabs",
      "activeTab",
      "storage",
      "unlimitedStorage",
      "contextMenus",
      "identity",
      "desktopCapture",
      "tabCapture"
   ],
   "short_name": "IBS Screen Recorder",
   "update_url": "https://bitst0rm.github.io/releases/bitstsadf0rm-chrome-extensions/Nimbus-Screenshot-Screen-Video-Recorder/update.xml",
   "version": "1.0",
   "web_accessible_resources": ["images/*"]
}