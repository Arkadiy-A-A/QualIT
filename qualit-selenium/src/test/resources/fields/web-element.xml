<?xml version="1.0" encoding="UTF-8"?>
<Pages xmlns="http://qualit.ibs-qa.ru/schema/web-elements">
    <Page name="Страница с фреймами">
        <!-->Поле находится внутри фрейма</-->
        <StaticText name="Заголовок" frames="//iframe[@src='header.html']" locator="//b" isLoaded="true"/>
        <Link name="Преимущества плавающих фреймов" frames="//iframe[@src='lefter.html']"
              locator="//a[text()='Преимущества плавающих фреймов']"/> <!-->Поле находится внутри другого фрейма</-->
        <Collection name="Варианты" frames="//iframe[@src='righter.html']" locator="//p">
            <Link name="Ссылка" locator=".//a"/>
        </Collection>
    </Page>
    <!-->Содержимое страницы находится внутри фрейма</-->
    <Page name="Левый фрейм" frames="//iframe[@src='lefter.html']" driver="ieDriver">
        <!-->Поле находится внутри другого фрейма, идем к нему через вышестоящий фрейм</-->
        <StaticText name="Заголовок" frames="..>//iframe[@src='header.html']" locator="//b"/>
        <Link name="Главная" locator="//a[text()='Главная']" isLoaded="true"/>
        <Link name="Преимущества плавающих фреймов" locator="//a[text()='Преимущества плавающих фреймов']"/>
    </Page>
    <!-->Содержимое страницы находится внутри фрейма</-->
    <Page name="Правый фрейм" frames="//iframe[@src='righter.html']" driver="ieDriver">
        <Collection name="Варианты" locator="//p">
            <Link name="Ссылка" locator=".//a"/>
        </Collection>
    </Page>
</Pages>