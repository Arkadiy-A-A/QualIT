<?xml version="1.0" encoding="UTF-8"?>
<Pages xmlns="http://qualit.ibs-qa.ru/schema/web-elements">
    <Page name="Банк Открытие" locator="" driver="Winium">
        <Button name="Перейти" locator="/*[@Name='Web-Монитор']//*[@Name='Перейти...']" isLoaded="true"/>
        <Button name="Отмена" locator="/*[@Name='Web-Монитор']//*[@Name='Отмена']"/>
        <TextInput name="Адресная строка (БО)" locator="/*[@Name='Web-Монитор']//*[@Name='Отмена']"/>
        <Page name="Информация о счетах клиента" locator="//legend[contains(text(),'Информация о счетах клиента')]/parent::fieldset" driver="Chrome">
            <Collection name="Список счетов" locator=".//div[@id='result3']/table[@class='tableborder']/tbody/tr[position()>1]">
                <StaticText name="Счет" locator="./td[1]"/>
                <TextInput name="Наименование" locator="./td[2]"/>
                <Button name="yery" locator="rrr" customType="custom!"/>
            </Collection>
        </Page>
    </Page>
    <Page name="Стартовая страница Яндекс" locator="">
        <Block name="Блок поиска"/>
        <TextInput name="Поиск" locator="//*[@aria-label='Запрос']" isLoaded="true"/>
        <Collection name="Сейчас в СМИ" locator="//div[@id='news_panel_news']/div/ol[not(contains(@class,'animation'))]/li">
            <StaticText name="Текст новости" locator="./a[contains(@class,'#{class}')]"/>
        </Collection>
        <Page name="Результаты поиска" locator="">
            <StaticText name="Результат поиска" locator="//*[@class='entity-search__header']/*[contains(@class, 'entity-search__title')]"/>
        </Page>
    </Page>
    <Page name="Блок поиска">
        <Button name="Найти" locator="//button[.//text()='Найти']"/>
    </Page>
</Pages>